---
import Label from "./Label.astro";
---

<div class="flex space-x-3">
  <Label name="Neovim">
    <img
      src="https://upload.wikimedia.org/wikipedia/commons/thumb/3/3a/Neovim-mark.svg/984px-Neovim-mark.svg.png"
      alt="Neovim logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Figma">
    <img
      src="https://upload.wikimedia.org/wikipedia/commons/thumb/3/33/Figma-logo.svg/langfr-220px-Figma-logo.svg.png"
      alt="Figma logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Docker">
    <img
      src="https://cdn4.iconfinder.com/data/icons/logos-and-brands/512/97_Docker_logo_logos-512.png"
      alt="Docker logo"
      class="h-[20px]"
    />
  </Label>
  <Label name="Github">
    <img
      src="https://upload.wikimedia.org/wikipedia/commons/thumb/c/c2/GitHub_Invertocat_Logo.svg/400px-GitHub_Invertocat_Logo.svg.png"
      alt="Github logo"
      class="h-[16px]"
    />
  </Label>
</div>
