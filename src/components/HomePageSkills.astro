---
import Label from "./Label.astro";
---

<div class="flex space-x-3">
  <Label name="Rust">
    <img
      src="https://upload.wikimedia.org/wikipedia/commons/d/d5/Rust_programming_language_black_logo.svg"
      alt="Rust logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Go">
    <img
      src="https://seeklogo.com/images/G/go-logo-046185B647-seeklogo.com.png"
      alt="Go logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Typescript">
    <img
      src="https://upload.wikimedia.org/wikipedia/commons/thumb/4/4c/Typescript_logo_2020.svg/2048px-Typescript_logo_2020.svg.png"
      alt="Typescript logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Ocaml">
    <img
      src="https://cdn.icon-icons.com/icons2/2699/PNG/512/ocaml_logo_icon_170902.png"
      alt="Ocaml logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Java">
    <img
      src="https://static-00.iconduck.com/assets.00/java-icon-756x1024-si1cd6qx.png"
      alt="Java logo"
      class="h-[16px]"
    />
  </Label>
</div>

<div class="flex space-x-3 mt-2">
  <Label name="Python">
    <img
      src="https://cdn.freebiesupply.com/logos/large/2x/python-5-logo-png-transparent.png"
      alt="Python logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Clang">
    <img
      src="https://upload.wikimedia.org/wikipedia/commons/1/19/C_Logo.png"
      alt="Clang logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Dart">
    <img
      src="https://upload.wikimedia.org/wikipedia/commons/7/7e/Dart-logo.png"
      alt="Dart logo"
      class="h-[16px]"
    />
  </Label>
  <Label name="Swift">
    <img
      src="https://i.pinimg.com/originals/8f/50/63/8f50630ae0e1775196e4c270c573ce67.png"
      alt="Swift logo"
      class="h-[16px]"
    />
  </Label>
</div>
